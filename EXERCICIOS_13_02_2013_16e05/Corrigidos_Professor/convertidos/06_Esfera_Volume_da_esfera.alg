algoritmo "Esfera"
// Função :
// Autor :
// Data : 13/10/2012
// Seção de Declarações 
var

      raio, volume,PPI: real


inicio

 PPI := 3.1415

  escreval("Informe o valor do raio da esfera: ")
  leia(raio)
  volume <- 4/3 * PPI * exp(raio, 3)
  escreval("O volume da esfera informada é: ", volume)



fimalgoritmo
